<h1 class="mt-3">Posts</h1>
<span class="mr-5"
    ><a class="btn btn-danger" href="<%=posts_dashboard_path%>" role="button"
      >Crea tu posts aqui</a
    ></span>
<div class="container row ml-2"></div>
    <% @post.each do |post|%>
        <div class="card-deck">
        <div class="card">
            <a><img class="card-img-top" src="<%= post.image_url%>" alt="image" /></a>
            <div class="card-body">
                <h2 class="card-title"><%= post.title %></h2>
                <p class="card-text"><%=post.content%></p>
                <p class="card-text"><%=post.created_at%></p>
            </div>
        </div>
        </div>
    <%end%>
</div>
